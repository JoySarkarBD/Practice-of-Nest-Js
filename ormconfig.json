{
  "type": "mongodb",
  "host": "127.0.0.1",
  "port": 27017,
  "database": "nest",
  "useUnifiedTopology": true,
  "entities": [
    "dist/**/*.entity.js"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  },
  "migrations": [
    "src/migration/**/*.ts"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ]
}